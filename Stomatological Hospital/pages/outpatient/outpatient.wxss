/* pages/outpatient/outpatient.wxss */

page {
  height: 100%;
  overflow: hidden;
}


.clinc{
  position: relative;
  height: 100%;
  overflow: hidden;
  display: flex;
  align-items: flex-end;
  justify-content: center;
  color: white;
}
.bg{
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: -1;
}
.menu{
  width: 100%;
  display: flex;
  justify-content: space-around;
  margin-bottom: 70rpx;
}
.menu>view{
  display: flex;
  flex-direction: column;
  text-align: center;
}
.menu>view>image{
  width: 120rpx;
  height: 120rpx;

}
.menu>view>text{
  font-size: 30rpx;
  font-weight: bold
}